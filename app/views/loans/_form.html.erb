<%= form_with model:@loan, local: true do |f| %>

<%= @current_user.name %>
<br>
<%= f.label :game_id %>
<%= f.collection_select :game_id, Game.all, :id, :title%>
<br>
<%= f.label :start_date %>
<%= f.date_field :start_date %>
<br>
<%= f.label :end_date %>
<%= f.date_field :end_date %>
<br>
<%= f.label :status %>
<%= f.text_field :status %>
<br>
<%= f.submit %>
<% end %>
