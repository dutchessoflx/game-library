<h1>Login</h1>

<% if flash[:error] %>
<br>
<strong><%= flash[:error] %></strong>
<% end %>
<br>
<br>

<%= form_with url: login_path, local:true do |f| %>
<%= f.label :contact %>
<%= f.text_field :contact %>
<br>
<%= f.label :password %>
<%= f.password_field :password %>
<br>
<%= f.submit "Login" %>
<% end %>
