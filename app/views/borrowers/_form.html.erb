<%= form_with model:@borrower, local:true, method: 'post' do |f| %>

<%= f.label :user_id, "Borrowed by User:"%>
<%= f.collection_select :user_id, User.all, :id, :name %>
<br>
<%= f.label :game_id, "Game borrowed:" %>
<%= f.collection_select :game_id, Game.all, :id, :title %>
<br>
<%= f.label :start_date %>
<%= f.date_field :start_date %>
<%= f.label :end_date %>
<%= f.date_field :end_date %>
<br><%= f.submit%>
<% end %>
