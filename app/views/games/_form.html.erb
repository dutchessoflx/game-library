<%= form_with model:@game, local:true, html: {multipart: true} do |f| %>
<%= f.label :title %>
<%= f.text_field :title %>
<br>
<%= f.label :user_id %>
<%= f.collection_select :user_id, User.all, :id, :name, include_blank: true %>
<br>
<%= f.label :min_players %>
<%= f.text_field :min_players %>
<br>
<%= f.label :max_players %>
<%= f.text_field :max_players%>
<br>
<%= f.label :description %>
<%= f.text_field :description %>
<br>
<%= f.label :available %>
<%= f.text_field :available %>
<br>
<%= f.label :category_id %>
<%= f.collection_check_boxes :category_id, Category.all, :id, :name, include_hidden: false %> 
<br>


<br>
<%= f.label :contents %>
<%= f.text_field :contents %>
<br>
<%= f.label :missing_damage %>
<%= f.text_field :missing_damage %>
<br>
<%= f.label :box_art %>
<%= f.text_field :box_art %>
<%= f.file_field :box_art %>
<br>
<%= f.submit %>
<% end %>
