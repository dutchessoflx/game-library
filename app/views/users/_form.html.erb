<%= form_with model: @user, local: true do |f|%>


<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :contact %>
<%= f.text_field :contact %>
<br>
<%= f.label :password %>
<%= f.password_field :password %>
<br>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>
<br>
<%= f.submit "Sign Up" %>
<% end %>
